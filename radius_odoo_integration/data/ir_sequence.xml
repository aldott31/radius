<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- RADIUS Username Sequence: 445XXXXXX format -->
        <record id="seq_radius_username" model="ir.sequence">
            <field name="name">RADIUS Username Sequence</field>
            <field name="code">res.partner.radius.username</field>
            <field name="prefix">445</field>
            <field name="padding">6</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
