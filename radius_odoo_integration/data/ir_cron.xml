<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Scheduled Action: Move expired RADIUS users to expired IP pool -->
        <record id="ir_cron_move_expired_to_expired_pool" model="ir.cron">
            <field name="name">RADIUS: Move Expired Services to Expired Pool</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model._cron_move_expired_to_expired_pool()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True"/>
        </record>

    </data>
</odoo>
